{
  "name": "bejamas",
  "version": "0.0.0",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "vite": "^7.1.2"
  },
  "lint-staged": {
    "*.{js}": [
